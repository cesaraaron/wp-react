{
  "name": "wp-react",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "invariant": "2.2.2",
    "lodash": "4.17.4",
    "normalizr": "3.2.4",
    "prop-types": "15.6.0",
    "qs": "6.5.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "react-test-renderer": "16.1.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "wpapi": "1.1.2"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.js"]
  },
  "lint-staged": {
    "*.{js}": ["eslint --fix", "git add"],
    "*.{js,json,css}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
